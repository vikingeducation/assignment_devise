<!DOCTYPE html>
<html>
  <head>
    <title>AssignmentDevise</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="error"><%= flash[:error] if flash[:error] %></p>
    <p class="success"><%= flash[:success] if flash[:success] %></p>
    <%= yield %>
  </body>
</html>
